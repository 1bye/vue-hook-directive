{
  "name": "vue-hook-directive",
  "description": "Plugin for vue, which integrates features from hookable to vue directives and composable",
  "author": "1bye",
  "repository": "https://github.com/1bye/vue-hook-directive",
  "license": "MIT",
  "private": false,
  "version": "0.1.5",
  "type": "module",
  "files": [
    "dist"
  ],
